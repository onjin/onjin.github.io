&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-59"&gt;&lt;/a&gt;             &lt;span class="n"&gt;write&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="s1"&gt;''&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;   &lt;span class="c1"&gt;# send headers now if body was empty&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-60"&gt;&lt;/a&gt;     &lt;span class="k"&gt;finally&lt;/span&gt;&lt;span class="p"&gt;:&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-61"&gt;&lt;/a&gt;         &lt;span class="k"&gt;if&lt;/span&gt; &lt;span class="nb"&gt;hasattr&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;result&lt;/span&gt;&lt;span class="p"&gt;,&lt;/span&gt;&lt;span class="s1"&gt;'close'&lt;/span&gt;&lt;span class="p"&gt;):&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-62"&gt;&lt;/a&gt;         &lt;span class="n"&gt;result&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;close&lt;/span&gt;&lt;span class="p"&gt;()&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-63"&gt;&lt;/a&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-64"&gt;&lt;/a&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-65"&gt;&lt;/a&gt; &lt;span class="c1"&gt;# Change this to the directory above your site code.&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-66"&gt;&lt;/a&gt; &lt;span class="n"&gt;sys&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;path&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;append&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="s2"&gt;"/py"&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-67"&gt;&lt;/a&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-68"&gt;&lt;/a&gt; &lt;span class="c1"&gt;# Change mysite to the name of your site package&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-69"&gt;&lt;/a&gt; &lt;span class="n"&gt;os&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;environ&lt;/span&gt;&lt;span class="p"&gt;[&lt;/span&gt;&lt;span class="s1"&gt;'DJANGO_SETTINGS_MODULE'&lt;/span&gt;&lt;span class="p"&gt;]&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="s1"&gt;'pytest.settings'&lt;/span&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-70"&gt;&lt;/a&gt;
&lt;a name="rest_code_bbd5b1cfc0b542e5ab6443976df4975a-71"&gt;&lt;/a&gt; &lt;span class="n"&gt;run_with_cgi&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;django&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;core&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;handlers&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;wsgi&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;WSGIHandler&lt;/span&gt;&lt;span class="p"&gt;())&lt;/span&gt;
&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;p&gt;i odpalamy to by www :D na razie tyle ...&lt;/p&gt;
&lt;/div&gt;
