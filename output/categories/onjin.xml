&lt;a href="http://marekwywial.name/posts/blog20130813python-soap-server-flask-wsdl-generation.html#rest_code_dc375c0172c049c4adf42e97cdf94024-18"&gt;18&lt;/a&gt;
&lt;a href="http://marekwywial.name/posts/blog20130813python-soap-server-flask-wsdl-generation.html#rest_code_dc375c0172c049c4adf42e97cdf94024-19"&gt;19&lt;/a&gt;&lt;/pre&gt;&lt;/div&gt;&lt;/td&gt;&lt;td class="code"&gt;&lt;pre class="code python"&gt;&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-1"&gt;&lt;/a&gt;&lt;span class="kn"&gt;from&lt;/span&gt; &lt;span class="nn"&gt;time&lt;/span&gt; &lt;span class="kn"&gt;import&lt;/span&gt; &lt;span class="n"&gt;ctime&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-2"&gt;&lt;/a&gt;&lt;span class="kn"&gt;from&lt;/span&gt; &lt;span class="nn"&gt;flask&lt;/span&gt; &lt;span class="kn"&gt;import&lt;/span&gt; &lt;span class="n"&gt;Flask&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-3"&gt;&lt;/a&gt;&lt;span class="kn"&gt;from&lt;/span&gt; &lt;span class="nn"&gt;flaskext.enterprise&lt;/span&gt; &lt;span class="kn"&gt;import&lt;/span&gt; &lt;span class="n"&gt;Enterprise&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-4"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-5"&gt;&lt;/a&gt;&lt;span class="n"&gt;app&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="n"&gt;Flask&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;__name__&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-6"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-7"&gt;&lt;/a&gt;&lt;span class="n"&gt;enterprise&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="n"&gt;Enterprise&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;app&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-8"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-9"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-10"&gt;&lt;/a&gt;&lt;span class="k"&gt;class&lt;/span&gt; &lt;span class="nc"&gt;DemoService&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;enterprise&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;SOAPService&lt;/span&gt;&lt;span class="p"&gt;):&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-11"&gt;&lt;/a&gt;    &lt;span class="n"&gt;__soap_server_address__&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="s1"&gt;'/soap'&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-12"&gt;&lt;/a&gt;    &lt;span class="n"&gt;__soap_target_namespace__&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; &lt;span class="s1"&gt;'ns'&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-13"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-14"&gt;&lt;/a&gt;    &lt;span class="nd"&gt;@enterprise.soap&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;_returns&lt;/span&gt;&lt;span class="o"&gt;=&lt;/span&gt;&lt;span class="n"&gt;enterprise&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;_sp&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;String&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-15"&gt;&lt;/a&gt;    &lt;span class="k"&gt;def&lt;/span&gt; &lt;span class="nf"&gt;get_time&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="bp"&gt;self&lt;/span&gt;&lt;span class="p"&gt;):&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-16"&gt;&lt;/a&gt;        &lt;span class="k"&gt;return&lt;/span&gt; &lt;span class="n"&gt;ctime&lt;/span&gt;&lt;span class="p"&gt;()&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-17"&gt;&lt;/a&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-18"&gt;&lt;/a&gt;&lt;span class="k"&gt;if&lt;/span&gt; &lt;span class="n"&gt;__name__&lt;/span&gt; &lt;span class="o"&gt;==&lt;/span&gt; &lt;span class="s1"&gt;'__main__'&lt;/span&gt;&lt;span class="p"&gt;:&lt;/span&gt;
&lt;a name="rest_code_dc375c0172c049c4adf42e97cdf94024-19"&gt;&lt;/a&gt;    &lt;span class="n"&gt;app&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;run&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="n"&gt;host&lt;/span&gt;&lt;span class="o"&gt;=&lt;/span&gt;&lt;span class="s1"&gt;'127.0.0.1'&lt;/span&gt;&lt;span class="p"&gt;,&lt;/span&gt; &lt;span class="n"&gt;port&lt;/span&gt;&lt;span class="o"&gt;=&lt;/span&gt;&lt;span class="mi"&gt;5555&lt;/span&gt;&lt;span class="p"&gt;)&lt;/span&gt;
&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/div&gt;
