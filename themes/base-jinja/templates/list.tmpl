{#  -*- coding: utf-8 -*- #}
{% extends 'base.tmpl' %}
{% block content %}
        <!--Body content-->
        <div class="postbox">
        <h1>{{ title }}</h1>
        <ul class="unstyled">
        {% for text, link, count in items %}
            <li><a href="{{ link }}">{{ text }} ({{ count }})</a>
        {% endfor %}
        </ul>
        </div>
        <!--End of body content-->
{% endblock %}
